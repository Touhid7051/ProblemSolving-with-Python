    "import turtle\n",
    "def square(sl):\n",
    "    for i in range(4):\n",
    "        turtle.forward(sl)\n",
    "        turtle.left(90)\n",
    "c=0\n",
    "while c<90:\n",
    "    square(100)\n",
    "    turtle.right(4)\n",
    "    c+=1\n",
    "turtle.exitonclick()\n"
